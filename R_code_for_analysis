t.test(aerocomp$ALW_new, y=aerocomp$ALW_amm, alternative="g", mu=0, conf.level=0.95)
aerocomp$ammdiff <- (aerocomp$ALW_amm-aerocomp$ALW_new)/((aerocomp$ALW_new+aerocomp$ALW_amm)/2)*100
summary(aerocomp$ammdiff, na.rm=T)

for(i in aerocompsites){
  site <- subset(aerocomp, AERONET_Site==i)
  a <- summary(site$ammdiff, na.rm=T)
  print(c(i,a))
}


###FIG S2 kappa comparison for organic ALW exclusion ####
gsfc <- subset(aeroimp, AERONET_Site=="GSFC")
gsfc$Year2 <- ifelse(gsfc$Month==12, gsfc$Year+1, gsfc$Year)
YrQtr <- as.yearqtr(as.yearmon(gsfc$Date, "%Y-%m-%d") + 1/12)
gsfc$Season <- as.numeric(factor(format(YrQtr, "%q"), levels=1:4, labels=c("1","2","3","4")))
gsfc$YrQtr <- as.yearqtr(as.yearmon(gsfc$Date, "%Y-%m-%d") + 1/12)

gsfc$ratio <- ifelse(gsfc$Season==1, 1.306703,
                     ifelse(gsfc$Season==2, 1.276604,
                            ifelse(gsfc$Season==3, 2.055942,
                                   ifelse(gsfc$Season==4, 1.434469, NA))))


gsfc$OM <- (gsfc$OCtot * gsfc$ratio)
gsfc$OM_gcm3 <- gsfc$OM*(1/10^6)*(1/10^6)
gsfc$Vo <- gsfc$OM_gcm3/1.4
gsfc$aw_1aw <- (gsfc$rh/(1-gsfc$rh))
gsfc$Vwo_k3 <- (gsfc$Vo)*(0.3)*(gsfc$aw_1aw) #using a korg value of 0.3 for rural location aerosol
gsfc$Vwo_ugm3_k3 <- (gsfc$Vwo_k3*(10^6)*(10^6))
gsfc$Vwo_k05 <- (gsfc$Vo)*(0.05)*(gsfc$aw_1aw) #opposite end korg value of 0.05
gsfc$Vwo_ugm3_k05 <- (gsfc$Vwo_k05*(10^6)*(10^6))

gsfccomp <- data.frame(Site=gsfc$AERONET_Site,Date=gsfc$Date,SO4=gsfc$SO4,NO3=gsfc$NO3,Na=gsfc$NAf,Chl=gsfc$CHL,Ca=gsfc$CA,K=gsfc$K,
                       Mg=gsfc$MG,OM=gsfc$OM,ALW=gsfc$ALW,ALW_org05=gsfc$Vwo_ugm3_k05)
gsfccomp[is.na(gsfccomp)] <- 0
gsfccomp$Dust_NaCl <- gsfccomp$Ca+gsfccomp$K+gsfccomp$Mg+gsfccomp$Na+gsfccomp$Chl
#drop individual ion columns for plotting
gsfccomp$Na<-NULL
gsfccomp$Chl<-NULL
gsfccomp$Ca<-NULL
gsfccomp$K<-NULL
gsfccomp$Mg<-NULL

gsfccomp <- aggregate(gsfccomp, by=list(gsfccomp$Site, gsfccomp$Date), FUN="mean", na.rm=T)
gsfccomp$Site <- gsfccomp$Group.1
gsfccomp$Group.1 <- NULL
gsfccomp$Group.2 <- NULL

gsfccomp <- gsfccomp[,c(1,2,4,7,6,5,8,3)]
gsfccomp <- gather(gsfccomp, NO3:SO4, key="species", value="value", factor_key=T)
gsfccomptots <- gsfccomp %>% dplyr::group_by(Site, Date) %>% dplyr::summarize(total=sum(value))
gsfccomp <- merge(gsfccomp, gsfccomptots, by=c("Site","Date"), all=T)

gsfccomp2 <- data.frame(Site=gsfc$AERONET_Site,Date=gsfc$Date,SO4=gsfc$SO4,NO3=gsfc$NO3,Na=gsfc$NAf,Chl=gsfc$CHL,Ca=gsfc$CA,K=gsfc$K,
                        Mg=gsfc$MG,OM=gsfc$OM,ALW=gsfc$ALW,ALW_org3=gsfc$Vwo_ugm3_k3)
gsfccomp2[is.na(gsfccomp2)] <- 0
gsfccomp2$Dust_NaCl <- gsfccomp2$Ca+gsfccomp2$K+gsfccomp2$Mg+gsfccomp2$Na+gsfccomp2$Chl
#drop individual ion columns for plotting
gsfccomp2$Na<-NULL
gsfccomp2$Chl<-NULL
gsfccomp2$Ca<-NULL
gsfccomp2$K<-NULL
gsfccomp2$Mg<-NULL

gsfccomp2 <- aggregate(gsfccomp2, by=list(gsfccomp2$Site, gsfccomp2$Date), FUN="mean", na.rm=T)
gsfccomp2$Site <- gsfccomp2$Group.1
gsfccomp2$Group.1 <- NULL
gsfccomp2$Group.2 <- NULL

gsfccomp2 <- gsfccomp2[,c(1,2,4,7,6,5,8,3)]
gsfccomp2 <- gather(gsfccomp2, NO3:SO4, key="species", value="value", factor_key=T)
gsfccomp2tots <- gsfccomp2 %>% dplyr::group_by(Site, Date) %>% dplyr::summarize(total=sum(value))
gsfccomp2 <- merge(gsfccomp2, gsfccomp2tots, by=c("Site","Date"), all=T)


g1 <- ggplot(gsfccomp, aes(x=Date, y=value, fill=species))+ylab("Fraction")+
  geom_bar(aes(fill=species), position="fill", stat="identity")+geom_text(aes(as.Date("2015-10-01"), 0.95, label="(a)"))+
  scale_fill_manual("", values=c("blue","turquoise4","skyblue1","green4","saddlebrown","red"),
                    labels=c("Nitrate",expression("ALW"[org]),"ALW","OM","Dust, NaCl","Sulfate"))+
  scale_x_date("Date", date_breaks="1 year", date_labels="%Y", limits=c(as.Date("2006-01-01"),as.Date("2015-10-31")))+
  theme(panel.grid=element_blank(), panel.background=element_blank(), axis.line=element_line(), legend.text=element_text(hjust=0), 
        axis.text=element_text(color="black"), legend.position="top", axis.ticks=element_line(),plot.title=element_text(size=12), 
        legend.key.size=unit(0.2, "in"), legend.key=element_blank()) + guides(fill=guide_legend(nrow=1))
g2 <- ggplot(gsfccomp2, aes(x=Date, y=value, fill=species))+ylab("Fraction")+
  geom_bar(aes(fill=species), position="fill", stat="identity")+geom_text(aes(as.Date("2015-10-01"), 0.95, label="(b)"))+
  scale_fill_manual("", values=c("blue","turquoise4","skyblue1","green4","saddlebrown","red"),
                    labels=c("Nitrate",expression("ALW"[org]),"ALW","OM","Dust, NaCl","Sulfate"))+
  scale_x_date("Date", date_breaks="1 year", date_labels="%Y", limits=c(as.Date("2006-01-01"),as.Date("2015-10-31")))+
  theme(panel.grid=element_blank(), panel.background=element_blank(), axis.line=element_line(), legend.text=element_text(hjust=0),
        axis.text=element_text(color="black"), legend.position="top", axis.ticks=element_line(),plot.title=element_text(size=12), 
        legend.key.size=unit(0.2, "in"), legend.key=element_blank()) + guides(fill=guide_legend(nrow=1))
g3 <- g1 + g2 + plot_layout(ncol=1, nrow=2, guides='collect') & theme(legend.position="top")
mypath <- file.path("D:/MMF/eta2/plots/", paste("gsfc_kappa_comps06-15.png"))
png(filename=mypath, width=7, height=5, units="in", res=300)
print(g3)
dev.off()


#calculating % of mass that organic ALW takes up from total PM2.5 mass (ALW included) & from total ALW mass
for(i in aerocompsites){
  site <- subset(aerocomp, AERONET_Site==i & Year>=2006 & Year<2016)
  a <- mean(site$Vwo_ugm3_k05/(site$Vwo_ugm3_k05+site$ALW_new)*100, na.rm=T)
  b <- median(site$Vwo_ugm3_k05/(site$Vwo_ugm3_k05+site$ALW_new)*100, na.rm=T)
  c <- mean(site$Vwo_ugm3_k3/(site$Vwo_ugm3_k3+site$ALW_new)*100, na.rm=T)
  d <- median(site$Vwo_ugm3_k3/(site$Vwo_ugm3_k3+site$ALW_new)*100, na.rm=T)
  e <- median(site$Vwo_ugm3_k05/(site$PM2.5+site$ALW_new+site$Vwo_ugm3_k05)*100, na.rm=T)
  f <- median(site$Vwo_ugm3_k3/(site$PM2.5+site$ALW_new+site$Vwo_ugm3_k3)*100, na.rm=T)
  print(c(i,"0.05",a,b,e))
  print(c(i,"0.3",c,d,f))
}


## AOD vs various PM2.5 FIGURE S5 ####
pm <- ggplot(data=subset(aerocomp, Year>=2006 & Year<2016), aes(PM2.5+NO3_v+ALW_new, AOD_440nm))+
  xlab(expression("Mass Concentration"~(mu*g~m^{-3})))+ylab(expression("AOD (440nm)"))+
  geom_point(aes(shape='c1', color='c1'), alpha=0.5)+
  geom_point(aes(PM2.5+ALW, AOD_440nm, shape='c2', color='c2'), alpha=0.3)+
  geom_point(aes(PM2.5+NO3_v, AOD_440nm, shape='c3', color='c3'), alpha=0.3)+
  geom_point(aes(PM2.5, AOD_440nm, shape='c4', color='c4'), alpha=0.3)+
  stat_regline_equation(label.y.npc="top",label.x.npc="left", aes(x=PM2.5+NO3_v+ALW_new, label=paste(..eq.label..,..rr.label.., sep="~~~")), color="black")+
  stat_regline_equation(label.y.npc=0.905,label.x.npc="left", aes(x=PM2.5+ALW_new, label=paste(..eq.label..,..rr.label.., sep="~~~")), color="mediumblue")+
  stat_regline_equation(label.y.npc=0.85,label.x.npc="left", aes(x=PM2.5+NO3_v, label=paste(..eq.label..,..rr.label.., sep="~~~")), color="olivedrab3")+
  stat_regline_equation(label.y.npc=0.795,label.x.npc="left", aes(x=PM2.5, label=paste(..eq.label..,..rr.label.., sep="~~~")), color="darkorange")+
  scale_shape_manual("", values=c(1,2,3,4), labels=c(expression("PM"[2.5]~"+ "*Delta*"NO"[3]~"+ ALW"),expression("PM"[2.5]~"+ ALW"),
                                                     expression("PM"[2.5]~"+ "*Delta*"NO"[3]),expression("PM"[2.5])))+
  scale_color_manual("", values=c("black","mediumblue","olivedrab3","darkorange"), 
                     labels=c(expression("PM"[2.5]~"+ "*Delta*"NO"[3]~"+ ALW"),expression("PM"[2.5]~"+ ALW"),expression("PM"[2.5]~"+ "*Delta*"NO"[3]),
                              expression("PM"[2.5])))+
  theme_classic(base_size=12)+theme(legend.text.align=0, axis.text=element_text(color="black"))
mypath <- file.path("D:/MMF/eta2/plots/sites/", paste("pm25vAOD.png"))
png(filename=mypath, width=6, height=4, units="in", res=300)
print(pm)
dev.off()

#stats
a <- lm(AOD_440nm ~ PM2.5, data=subset(aerocomp, Year>=2006 & Year<2016))
b <- lm(AOD_440nm ~ (PM2.5+ALW_new), data=subset(aerocomp, Year>=2006 & Year<2016))
c <- lm(AOD_440nm ~ (PM2.5+NO3_v), data=subset(aerocomp, Year>=2006 & Year<2016))
d <- lm(AOD_440nm ~ (PM2.5+ALW_new+NO3_v), data=subset(aerocomp, Year>=2006 & Year<2016))

anova(a,b)
t.test(b,c, alternative="two.sided")
anova(a,c)
anova(a,d)
anova(b,c)
anova(b,d)
anova(c,d)

a <- aov(AOD_440nm ~ PM2.5+NO3_v, data=subset(aerocomp, Year>=2006 & Year<2016))
summary(a)


for(i in aerocompsites){
  site <- subset(aerocomp, AERONET_Site==i & Year>=2006 & Year<2016)
  a <- lm(AOD_440nm ~ PM2.5, data=site)
  b <- lm(AOD_440nm ~ (PM2.5+ALW_new), data=site)
  c <- lm(AOD_440nm ~ (PM2.5+NO3_v), data=site)
  d <- lm(AOD_440nm ~ (PM2.5+ALW_new+NO3_v), data=site)
  e <- anova(a,b)
  f <- anova(a,c)
  g <- anova(a,d)
  h <- anova(b,c)
  j <- anova(b,d)
  k <- anova(c,d)
  print(i)
  print(e)
  print(f)
  print(g)
  print(h)
  print(j)
  print(k)
}


