  site1 <- subset(lwpmss, AERONET_Site==i & Year==2006)
  site2 <- subset(lwpmss, AERONET_Site==i & Year==2015)
  a <- summary(site1$AOD_440nm)
  b <- summary(site2$AOD_440nm)
  print(c(i,a,b))
}

## hit rate ####
for(i in aerocompsites){
  site <- subset(aerocomp, AERONET_Site==i)
  a <- summary(site$PM2.5+site$NO3_v)
  b <- summary(site$AOD_440nm)
  c <- summary(site$PM2.5+site$NO3_v+site$ALW_new)
  print(i)
  print(c)
  print(a)
  print(b)
}

## hit rate ####
for(i in aerocompsites){
  site <- subset(aerocomp, AERONET_Site==i & Year>=2006 & Year<2016)
  a <- summary(site$PM2.5+site$NO3_v)
  b <- summary(site$AOD_440nm)
  print(i)
  print(a)
  print(b)
}

hitrates <- read.table(text="Site	Site2	Site3	region	pmdry75	aod75	wetslope	dryslope	wetsig	drysig
Billerica	BILL	5	1	10.319	0.201	-0.07293	-0.0691	1	1
BONDVILLE	BOND	2	1	12.455	0.254	-0.01625	-0.0353	2	1
CCNY	CCNY	6	1	13.946	0.299	-0.03824	-0.0484	1	1
Fresno	FRES	7	2	22.514	0.194	-0.01232	-0.0109	2	2
GSFC	GSFC	3	1	14.452	0.282	-0.02564	-0.0441	1	1
MD_Science_Center	MDSC	4	1	14.224	0.298	-0.04163	-0.0480	1	1
Red_Mountain_Pass	RMNP	8	2	3.616	0.077	-0.02195	-0.0203	2	2
Sioux_Falls	SXFL	1	1	9.022	0.203	-0.04307	-0.0185	1	1", header=T)


for(i in aerocompsites){
  site <- subset(aerocomp, AERONET_Site==i & Year>=2006 & Year<2016)
  hit <- subset(hitrates, Site==i)
  a <- sum(site$PM2.5+site$NO3_v >= hit$pmdry75, na.rm=T)
  b <- sum(site$AOD_440nm >= hit$aod75, na.rm=T)
  c <- sum(site$PM2.5+site$NO3_v >= hit$pmdry & site$AOD_440nm >= hit$aod75, na.rm=T)
  d <- sum((site$PM2.5+site$NO3_v+site$ALW_new) >= hit$pmdry75, na.rm=T)
  e <- sum((site$PM2.5+site$NO3_v+site$ALW_new) >= hit$pmwet75 & site$AOD_440nm >= hit$aod75, na.rm=T)
  print(i)
  print(c("dry",a,b,c))
  print(c("wet",d,b,e))
}

#now do year by year
for(i in aerocompsites){
  site <- subset(aerocomp, AERONET_Site==i & Year>=2006 & Year<2016)
  hit <- subset(hitrates, Site==i)
  for(j in 2006:2015){
    site2 <- subset(site, Year==j)
    a <- sum(site2$PM2.5+site2$NO3_v >= hit$pmdry75, na.rm=T)
    b <- sum(site2$AOD_440nm >= hit$aod75, na.rm=T)
    c <- sum(site2$PM2.5+site2$NO3_v >= hit$pmdry & site2$AOD_440nm >= hit$aod75, na.rm=T)
    d <- sum((site2$PM2.5+site2$NO3_v+site2$ALW_new) >= hit$pmdry75, na.rm=T)
    e <- sum((site2$PM2.5+site2$NO3_v+site2$ALW_new) >= hit$pmdry75 & site2$AOD_440nm >= hit$aod75, na.rm=T)
    print(c(i,j))
    print(c("dry",a,b,c))
    print(c("wet",d,b,e))
  }
}


#hityrs <- read.table(text="hitrates tab on eta_new spreadsheet", header=T)
for(i in aerocompsites){
  corr <- subset(hityrs, Site==i)
  a <- lm(dryhr06_15~Year, data=corr, method="qr", na.action=na.omit)
  b <- lm(wethr06_15~Year, data=corr, method="qr", na.action=na.omit)
  c <- summary(a)
  d <- summary(b)
  print(i)
  print("wet")
  print(d)
  print("dry")
  print(c)
}

hitrates <- hitrates[,c(1:6,9:10,8,7)]
hitrateslong <- pivot_longer(hitrates, dryslope:wetslope, names_to="var", values_to="val")
hitrateslong$region2 <- as.factor(ifelse(hitrateslong$region==1, "Humid", 
                                        ifelse(hitrateslong$region==2, "Arid", NA)))
hitrateslong <- hitrateslong[order(hitrateslong$Site3),]
labs <- unique(hitrateslong$Site2)
hitrateslong$var <- as.factor(hitrateslong$var)

#plotting hit rate annual change
hryr <- ggplot(hitrateslong, aes(Site3, reorder(var, desc(var))))+xlab("")+
  scale_y_discrete("", labels=c("Adjusted","Reported"))+
  scale_x_continuous("", breaks=c(1,2,3,4,5,6,7,8), labels=labs)+
  geom_tile(aes(fill=val*100, group=var), width=0.95, height=0.95)+
  geom_polygon(aes(fill=val*100, group=var), stat="identity", position="identity")+
  geom_point(data=subset(hitrateslong, var=="wetslope"), aes(color=factor(wetsig), group=var), size=5, shape=17, show.legend=F)+
  geom_point(data=subset(hitrateslong, var=="dryslope"), aes(color=factor(drysig), group=var), size=5, shape=17, show.legend=F)+
  scale_fill_gradient2(expression(Delta*theta~("%"~yr^{-1})), limits=c(-6,0), low="blue", mid="#FFFFAA", high="red", 
                       midpoint=0, na.value="transparent", guide='colourbar', oob=squish)+
  scale_color_manual("", values=c("black","transparent"), labels=c("",""))+
  facet_grid(~region2, scales='free', space='free')+ theme_classic(base_size=13.5)+
  guides(size="none", fill=guide_colourbar(title.position="left"))+
  theme(panel.background=element_blank(), panel.grid.major=element_blank(), panel.grid.minor=element_blank(), legend.title.align=0.5,
        axis.text=element_text(color="black"), axis.title=element_blank(), strip.text=element_text(color="black"), 
        axis.line.x=element_line(color="black"), axis.ticks=element_blank(), axis.line.y=element_blank(), legend.title=element_text(angle=90),
        legend.key=element_blank(), legend.text=element_text(color="black"), legend.position="right", axis.text.y=element_text(angle=90, hjust=0.5), 
        legend.direction="vertical")
mypath <- file.path("D:/MMF/eta2/plots/", paste("hitrate_annualchange_tile_new06-15.png"))
png(filename=mypath, width=6, height=2.5, units="in", res=300)
print(hryr)
dev.off()



## f(RH) CALCULATIONS ####
aerocomp$OCtot <- ifelse(is.na(aerocomp$OCtot), 0, aerocomp$OCtot)
aerocomp$OM <- ifelse(is.na(aerocomp$OM), 0, aerocomp$OM)
aerocomp$PM_spec <- aerocomp$NO3 + aerocomp$SO4 + aerocomp$OM + aerocomp$NAf + aerocomp$CHL + 
                        aerocomp$CA + aerocomp$K + aerocomp$MG

# keep everything in ug/m3 or g/cm3
# conversion: 1 g/cm3 = 1*10^12 ug/m3
aerocomp$dry_rho <- ((1.725)*aerocomp$NO3 + (1.10)*aerocomp$SO4 + (1.4)*aerocomp$OM + (2.17)*(aerocomp$NAf+aerocomp$CHL) +
                        (1.55)*aerocomp$CA + (0.862)*aerocomp$K + (1.738)*aerocomp$MG)/(aerocomp$PM_spec)

#now do the r1/r2 equation (piecemeal for sanity)
aerocomp$r1 <- (aerocomp$ALW_new/(1.0))^(1/3) + (aerocomp$PM2.5/aerocomp$dry_rho)^(1/3)
aerocomp$r2 <- (aerocomp$PM2.5/aerocomp$dry_rho)^(1/3)
aerocomp$r1_r2 <- aerocomp$r1 / aerocomp$r2

aerocomp <- drop_na(aerocomp, PM2.5, AOD_440nm)

for(i in aerocompsites){
  site <- subset(aerocomp, AERONET_Site==i)
  a <- max(site$pbl, na.rm=T)
  print(i)
  print(a)
}
#pbls <- read.table(text="pblmax in site_matching spreadsheet sitematch tab", header=T)
aerocomp <- merge(aerocomp, pbls, by="AERONET_Site")
aerocomp$pblnorm_site <- aerocomp$pbl / aerocomp$pblmax

## FIGURE TWO ####
arid <- subset(aerocomp, region==2)
humid <- subset(aerocomp, region==1)

arid$smooth_PM2.5 <- predict(loess(PM2.5 ~ as.numeric(Date), arid, span=0.05), newdata=arid$Date)
arid$UB_PM  <-  predict(loess(PM2.5+NO3_v ~ as.numeric(Date), arid, span=0.05), newdata=arid$Date)
humid$smooth_PM2.5 <- predict(loess(PM2.5 ~ as.numeric(Date), humid, span=0.05), newdata=humid$Date)
humid$UB_PM  <-  predict(loess(PM2.5+NO3_v ~ as.numeric(Date), humid, span=0.05), newdata=humid$Date)
arid$smooth_NO3 <- predict(loess(NO3 ~ as.numeric(Date), arid, span=0.05), newdata=arid$Date)
arid$UB_NO3 <- predict(loess(NO3+NO3_v ~ as.numeric(Date), arid, span=0.05), newdata=arid$Date)
humid$smooth_NO3 <- predict(loess(NO3 ~ as.numeric(Date), humid, span=0.05), newdata=humid$Date)
humid$UB_NO3 <- predict(loess(NO3+NO3_v ~ as.numeric(Date), humid, span=0.05), newdata=humid$Date)



