  site <- subset(aerocomp2006, AERONET_Site==i)
  a <- summary(site$PM2.5)
  b <- summary(site$SO4)
  c <- summary(site$NO3+site$NO3_v)
  d <- summary(site$ALW_new)
  e <- summary(site$AOD_440nm)
  print(i)
  print(a)
  print(b)
  print(c)
  print(d)
  print(e)
}

