for(i in aerocompsites){
  site <- subset(lwpmss, AERONET_Site==i & Year==2006)
  a <- summary(site$SO4, na.rm=T)
  b <- summary(site$NAf+site$CHL+site$CA+site$K+site$MG, na.rm=T)
  c <- summary(site$OM, na.rm=T)
  f <- summary(site$ALW_new, na.rm=T)
  g <- summary(site$NO3, na.rm=T)
  print(i)
  print(c("SO4",a))
  print(c("Dust",b))
  print(c("OM",c))
  print(c("ALW",f))
  print(c("NO3",g))
}

