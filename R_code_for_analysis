## change in species over time
for(i in aerocompsites){
  site <- subset(lwpmss, AERONET_Site==i & Year>=2006 & Year<2016)
  a <- lm((site$PM2.5)~site$Date, data=site, method="qr", na.action=na.omit)
  b <- lm(site$SO4~site$Date, data=site, method="qr", na.action=na.omit)
  c <- lm((site$NO3)~site$Date, data=site, method="qr", na.action=na.omit)
  d <- lm(site$ALW_new~site$Date, data=site, method="qr", na.action=na.omit)
  e <- lm(site$AOD_440nm~site$Date, data=site, method="qr", na.action=na.omit)
  f <- lm((site$NO3+site$NO3_v)~site$Date, data=site, method="qr", na.action=na.omit)
  print(i)
  print(summary(a))
  print(summary(b))
  print(summary(c))
  print(summary(d))
  print(summary(e))
  print(summary(f))
}


for(i in 1:2){
  site <- subset(lwpmss, region==i & Year>=2006 & Year<2016)
  a <- lm((site$PM2.5)~site$Date, data=site, method="qr", na.action=na.omit)
  b <- lm(site$SO4~site$Date, data=site, method="qr", na.action=na.omit)
  c <- lm((site$NO3)~site$Date, data=site, method="qr", na.action=na.omit)
  d <- lm(site$ALW_new~site$Date, data=site, method="qr", na.action=na.omit)
  e <- lm(site$AOD_440nm~site$Date, data=site, method="qr", na.action=na.omit)
  f <- lm((site$NO3+site$NO3_v)~site$Date, data=site, method="qr", na.action=na.omit)
  print(i)
  print(summary(a))
  print(summary(b))
  print(summary(c))
  print(summary(d))
  print(summary(e))
  print(summary(f))
}


